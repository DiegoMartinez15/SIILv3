            <!--Menu de Producto--> \r\n            <v-list-item link :to=\"{name: 'productos'}\">\r\n              <v-list-item-action>\r\n                <v-icon>laptop</v-icon>\r\n              </v-list-item-action>\r\n              <v-list-item-content>\r\n                <v-list-item-title>Productos</v-list-item-title>\r\n              </v-list-item-content>\r\n            </v-list-item> \r\n      </v-list-group>\r\n      <!--Opciones de pedido-->\r\n            <template> <!--opcion de pedidos -->\r\n                <v-list-item link :to=\"{name:'pedidos.listado.recibidos'}\"> <!-- MENU DE productos-->\r\n                  <v-list-item-action>\r\n                    <v-icon>shopping_cart</v-icon>\r\n                  </v-list-item-action>\r\n                <v-list-item-content>\r\n                  <v-list-item-title>Pedidos</v-list-item-title>\r\n                </v-list-item-content>\r\n              </v-list-item>\r\n            </template> \r\n\r\n\r\n\r\n        <!--menu para seguridad-->\r\n        <v-list-group>\r\n          <template slot=\"activator\">\r\n                <v-list-item-action>\r\n                    <v-icon>settings</v-icon>\r\n                </v-list-item-action>\r\n                <v-list-item-content>\r\n                  <v-list-item-title>Gestion de Usuarios</v-list-item-title>\r\n                </v-list-item-content>\r\n          </template>\r\n          <v-list-item link ><!--menu de roles-->\r\n              <v-list-item-action>\r\n                <v-icon>person</v-icon>\r\n              </v-list-item-action>\r\n              <v-list-item-content>\r\n                <v-list-item-title>Roles</v-list-item-title>\r\n              </v-list-item-content>\r\n            </v-list-item>\r\n            <v-list-item link> <!-- MENU DE MARCAS-->\r\n              <v-list-item-action>\r\n                <v-icon>group</v-icon>\r\n              </v-list-item-action>\r\n              <v-list-item-content>\r\n                <v-list-item-title>Usuarios</v-list-item-title>\r\n              </v-list-item-content>\r\n            </v-list-item>\r\n        </v-list-group>\r\n        \r\n        <!--Menu de Repostes-->\r\n        <v-list-group> <!--menu desplegable para reporte-->\r\n          <template slot=\"activator\">\r\n                <v-list-item-action>\r\n                    <v-icon>insert_chart</v-icon>\r\n                </v-list-item-action>\r\n                <v-list-item-content>\r\n                  <v-list-item-title>Reportes</v-list-item-title>\r\n                </v-list-item-content>\r\n          </template>\r\n          <v-list-item link ><!--menu de roles-->\r\n            <v-list-item-action>\r\n              <v-icon>picture_as_pdf</v-icon>\r\n            </v-list-item-action>\r\n            <v-list-item-content>\r\n              <v-list-item-title>Pedidos por periodo</v-list-item-title>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n          <v-list-item link> <!-- MENU DE pedido despachados-->\r\n            <v-list-item-action>\r\n              <v-icon>picture_as_pdf</v-icon>\r\n            </v-list-item-action>\r\n            <v-list-item-content>\r\n              <v-list-item-title>Pedidos despachados</v-list-item-title>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n          <v-list-item link> <!-- MENU DE pedido anuilados-->\r\n            <v-list-item-action>\r\n              <v-icon>picture_as_pdf</v-icon>\r\n            </v-list-item-action>\r\n            <v-list-item-content>\r\n              <v-list-item-title>Pedidos anulados</v-list-item-title>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n        </v-list-group>\r\n      </v-list>\r\n    \r\n    </v-navigation-drawer>\r\n\r\n    <v-app-bar\r\n      :clipped-left=\"$vuetify.breakpoint.lgAndUp\"\r\n      app\r\n      color=\"blue darken-3\"\r\n      dark\r\n    >\r\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\r\n      <v-toolbar-title\r\n        style=\"width: 600px\"\r\n        class=\"ml-0 pl-4\"\r\n      >\r\n        <span class=\"hidden-sm-and-down\">Sistema de Intermediacion Laboral</span>\r\n      </v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-btn @click=\"logout()\" icon>\r\n        <v-icon\r\n        >mdi mdi-logout</v-icon>\r\n      </v-btn>\r\n    </v-app-bar>\r\n    <v-main>\r\n      <v-container\r\n        class=\"fill-height\"\r\n        fluid\r\n      >\r\n        <v-layout>\r\n          <v-flex>\r\n             <!-- Componentes Poner-->\r\n              <router-view/>\r\n          </v-flex>\r\n        </v-layout>       \r\n      </v-container>\r\n    </v-main>\r\n    <v-footer color=\"indigo\" app>\r\n      <span class=\"white--text\">&copy; {{new Date().getFullYear()}} - Derechos Reservados - ITCHA AGAPE</span>\r\n    </v-footer>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { mapState } from \"vuex\";\r\nexport default {\r\n  props: {\r\n      source: String,\r\n    },\r\n  name: 'Home',\r\n\r\n\r\n  components: {\r\n  },\r\n\r\n  data: () => ({\r\n    drawer: null,\r\n  }),\r\n  methods:{\r\n     logout(){\r\n             let x = localStorage.getItem('token');\r\n            let me = this,\r\n            header = {\r\n              headers: {\r\n                \"Authorization\": \"Bearer \"+ x,\r\n              },\r\n            };\r\n            console.log(x)\r\n             me.$http.get(`${me.$url}/logout`,header)\r\n            .then(function(response){ \r\n               if(response.status==200){ \r\n                  localStorage.clear();           \r\n                  alert(\"Adios\");\r\n                  me.$router.push('/login')\r\n                console.log(response.data);\r\n               }                        \r\n            })\r\n            .catch(function(error){              \r\n              console.log(error);\r\n            });\r\n          },\r\n\r\n  },\r\n  computed:{\r\n    \r\n      ...mapState(['name'])\r\n    },\r\n  mounted(){\r\n    let x =localStorage.getItem('token')\r\n      if(x != null){\r\n        console.log(\"Si tienes acceso\");\r\n      }else{\r\n         \r\n         this.$router.push('/login')\r\n      }\r\n       \r\n\r\n  },\r\n};\r\n</script>\r\n"]}]}