{"remainingRequest":"C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\src\\components\\FormAccept.vue?vue&type=style&index=0&id=1e84cf1c&scoped=true&lang=css&","dependencies":[{"path":"C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\src\\components\\FormAccept.vue","mtime":1607477825730},{"path":"C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\node_modules\\css-loader\\dist\\cjs.js","mtime":1605315235123},{"path":"C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1605315271037},{"path":"C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\node_modules\\postcss-loader\\src\\index.js","mtime":1605315252447},{"path":"C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1605315226084},{"path":"C:\\wamp64\\www\\SIILv3GitHub\\siil-front\\node_modules\\vue-loader\\lib\\index.js","mtime":1605315271037}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["FormAccept.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwRA;AACA;AACA;AACA;;AAEA","file":"FormAccept.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n<v-app  class=\"img\">\r\n  <div>\r\n   <v-card width=\"440\" class=\"mx-auto  mt-14  pl-5 pr-5 \">\r\n        <v-card-title class=\"justify-center pt-12 \"  id=\"title\">Proceso de Aceptacion</v-card-title>\r\n        <v-spacer></v-spacer>\r\n        <v-card-text>\r\n         <h4 class=\" dark--text text-center \">Acceptas El proceso de Intermedacion Laboral</h4>\r\n        </v-card-text>\r\n        <v-card-actions class=\"pt-6 pb-6\">\r\n            <v-row align=\"center\"\r\n            justify=\"space-around\">\r\n                <div>\r\n                    <v-btn\r\n                    @click=\"FormLoad()\"\r\n                     color=\"info\"> <v-icon left>\r\n                    mdi mdi-checkbox-multiple-marked-circle\r\n                  </v-icon>SI</v-btn>\r\n                </div>\r\n                <div >\r\n                    <v-btn\r\n                    @click=\"no = true\"\r\n                     color=\"error\">\r\n                    <v-icon left>\r\n                    mdi mdi-close\r\n                    </v-icon>No</v-btn>\r\n                </div>\r\n            </v-row>\r\n        </v-card-actions>\r\n        <v-card-actions class=\"mt \" v-if=\"no == true\">\r\n            <v-container fluid>\r\n                <h3><v-icon left>\r\n                    mdi mdi-emoticon-sad\r\n                    </v-icon>&nbsp;&nbsp;Por que no?</h3>\r\n                <v-row align=\"center\"\r\n                    class=\"pl-10\"\r\n                    justify=\"space-around\">\r\n                    \r\n              \r\n          \r\n                  <v-col\r\n                      cols=\"12\"\r\n                      sm=\"6\"\r\n                      md=\"6\"\r\n                    >\r\n                  <v-radio-group v-model=\"radioGroup\">\r\n                    <v-radio\r\n                    @click=\"Campo = false\"\r\n                    label=\"Trabajare\"\r\n                    value=\"Trabajare\"\r\n                    ></v-radio>\r\n                     <v-radio\r\n                     @click=\"Campo = false\"\r\n                     checked\r\n                    label=\"Seguire estudiando\"\r\n                    value=\"Seguire estudiando\"\r\n                      ></v-radio>\r\n                  </v-radio-group>\r\n                  </v-col>\r\n                  <v-col\r\n                      cols=\"12\"\r\n                      sm=\"6\"\r\n                      md=\"6\">\r\n                <v-radio-group v-model=\"radioGroup\">\r\n                   \r\n                    <v-radio\r\n                     @click=\"Campo = false\"\r\n                    label=\"Emigrare\"\r\n                    value=\"Emigrare\"\r\n                    \r\n                    \r\n                    ></v-radio>\r\n                     <v-radio\r\n                     @click=\"Campo = true\"\r\n                      label=\"Otro\"\r\n                      value=\"Otro\"\r\n                    ></v-radio>\r\n                   \r\n                </v-radio-group>\r\n                </v-col>\r\n                \r\n               </v-row>\r\n                <v-row >\r\n                  <v-col\r\n                  cols=\"12\"\r\n                  >\r\n                  <div v-show=\"Campo == true\">\r\n                 <v-textarea\r\n                  v-model=\"form.porque\"\r\n                  label=\"Otras situaccion\"\r\n                  \r\n                  auto-grow\r\n                  outlined\r\n                  rows=\"1\"\r\n                  row-height=\"15\"\r\n                ></v-textarea>\r\n                  </div>\r\n                </v-col>\r\n                 \r\n                </v-row>\r\n               \r\n               <v-btn\r\n                   \r\n                    @click=\"formNotAccept()\"\r\n                     color=\"success\"\r\n                     >\r\n                    <v-icon Rigth>\r\n                    mdi mdi-near-me\r\n                    </v-icon>Enviar</v-btn>\r\n            </v-container>\r\n        </v-card-actions>\r\n      </v-card>\r\n  </div>\r\n  <!--Modal de acceptacion-->\r\n  <template>\r\n  <v-row justify=\"center\">\r\n    <v-dialog\r\n     persistent\r\n      v-model=\"dialog\"\r\n      width=\"600px\"\r\n    >\r\n      <v-card>\r\n        <v-card-title>\r\n          <span class=\"headline\">Intermedacion Laboral</span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n        Intermedacion Laboral es proceso que Itcha proporcia a sus Estudiantes durante un a√±o,\r\n        ayudando abridar ofertas de empleo en el sector publico y privado.\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n            color=\"accent darken-3\"\r\n            text\r\n            @click=\"fetchUsuarios()\">\r\n            Accepto\r\n          </v-btn>\r\n          <v-btn\r\n            color=\"accent darken-3\"\r\n            text\r\n            @click=\"dialog = false\"\r\n          >\r\n            Cerrar\r\n          </v-btn>        \r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n  </v-row>\r\n  </template>\r\n  <!--Fin del Modal de acceptacion-->\r\n</v-app>\r\n</template>\r\n<script>\r\nexport default {\r\n    data () {\r\n      return {\r\n        radioGroup: 1,\r\n        no:false,\r\n        dialog: false,\r\n        Campo: false,\r\n        form:{\r\n           acepto: \"\",\r\n          porque: \"\",\r\n        },\r\n      };\r\n    },\r\n      methods:{\r\n          FormLoad(){\r\n            let me = this;\r\n             me.dialog = true;\r\n             me.no = false;\r\n          },\r\n          fetchUsuarios() {\r\n            let me = this;\r\n             me.form.acepto = \"SI\";\r\n            me.$http\r\n            .post(`${me.$url}/formfirst`, this.form,{\r\n              headers:{\r\n                Authorization: 'Bearer '+localStorage.getItem('token'),\r\n              }\r\n            } )\r\n            .then(function(response){\r\n               console.log(me.form);\r\n              me.dialog = false;\r\n              me.$swal({title:\"Bienvenido al SIIL\",position: 'center',\r\n                      icon: 'success',                     \r\n                      showConfirmButton: false,\r\n                      timer: 2500\r\n                    });\r\n                console.log(response.data);\r\n                    me.$router.push('/');\r\n            })\r\n            .catch(function(error){\r\n              console.log(error);\r\n            });\r\n          },\r\n           formAccept(){\r\n            let me = this; \r\n            me.form.acepto = \"SI\";\r\n            console.log(me.form);\r\n            me.$http\r\n              .post(`${me.$url}/formAccept`, me.form)\r\n              .then(function(response){\r\n                  console.log(response.data);\r\n                    me.dialog = false; \r\n                    me.$router.push('/');\r\n              })\r\n              .catch(function(error){\r\n                console.log(error);\r\n              });\r\n          },     \r\n\r\n\r\n          formNotAccept(){\r\n              let me = this;\r\n                  if(me.radioGroup == \"Otro\"){\r\n                    me.form.acepto = \"NO\";\r\n                    console.log(me.form);\r\n                  }else{\r\n                    me.form.acepto = \"NO\";\r\n                    me.form.porque = me.radioGroup;\r\n                  console.log(me.form);\r\n                  }\r\n              me.$http\r\n                .post(`${me.$url}/formfirst`,me.form,{\r\n              headers:{\r\n                Authorization: 'Bearer '+localStorage.getItem('token'),\r\n              }\r\n            } )\r\n                .then(function(response){\r\n                    console.log(response.data);\r\n                     me.logout();  \r\n                })\r\n                .catch(function(error){\r\n                  console.log(error);\r\n            });\r\n           \r\n          },\r\n          logout(){\r\n             let x = localStorage.getItem('token');\r\n            let me = this,\r\n            header = {\r\n              headers: {\r\n                \"Authorization\": \"Bearer \"+ x,\r\n              },\r\n            };\r\n             me.$http.get(`${me.$url}/logout`,header)\r\n            .then(function(response){ \r\n               if(response.status==200){ \r\n                  localStorage.clear();           \r\n                  me.$swal({\r\n                    title:\"No acepto el proceso de Intermedacion Laboral\",\r\n                    icon: 'info',\r\n                    timer: 3500,\r\n                    showConfirmButton: false,\r\n                    footer: 'Para mayor informacion comunicarse con el encargado'                     \r\n                    });\r\n                  me.$router.push('/login');\r\n               }                        \r\n            })\r\n            .catch(function(error){              \r\n              console.log(error);\r\n            });\r\n          }\r\n    },\r\n    mounted(){\r\n       let x =localStorage.getItem('token')\r\n      if(x != null){\r\n        console.log(\"Si tienes acceso\");\r\n      }else{\r\n         \r\n         this.$router.push('/login')\r\n      }\r\n       \r\n\r\n    },\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n.img{\r\n    background-image: url('/itcha/accept1.jpg');\r\n    background-repeat: no-repeat;\r\n    background-size: 100% 100%;\r\n    \r\n}\r\n\r\n</style>\r\n"]}]}