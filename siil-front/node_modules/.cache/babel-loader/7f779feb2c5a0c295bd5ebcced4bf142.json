CA,UAAA,KAAA,CAAA,IAAA,CAAA;AACA,YAAA,KAAA,EAAA,iCADA;AAEA,YAAA,IAAA,EAAA,uGAFA;AAGA,YAAA,IAAA,EAAA,SAHA;AAIA,YAAA,gBAAA,EAAA,IAJA;AAKA,YAAA,kBAAA,EAAA,SALA;AAMA,YAAA,iBAAA,EAAA,MANA;AAOA,YAAA,iBAAA,EAAA;AAPA,WAAA,EAQA,IARA,CAQA,UAAA,MAAA,EAAA;AACA,gBAAA,MAAA,CAAA,WAAA,EAAA;AACA,cAAA,KAAA,CAAA,IAAA,CACA,wBADA,EAEA,0CAFA,EAGA,SAHA;AAKA;AACA,WAhBA;AAiBA;AAnBA;AAqBA;AA3JA,GAlHA;AAgRA,EAAA,OAhRA,qBAgRA;AAEA,QAAA,CAAA,GAAA,cAAA,CAAA,OAAA,CAAA,QAAA,CAAA;;AACA,QAAA,CAAA,IAAA,IAAA,EAAA;AACA,WAAA,eAAA;AACA,KAFA,MAEA;AAEA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;;AACA,QAAA,EAAA,GAAA,IAAA;AAEA,IAAA,EAAA,CAAA,eAAA;AACA,IAAA,EAAA,CAAA,aAAA;AACA,IAAA,EAAA,CAAA,YAAA;AACA,IAAA,EAAA,CAAA,YAAA;AACA,IAAA,EAAA,CAAA,iBAAA;AACA;AAhSA,CAAA","sourcesContent":["\n<template>\n  <v-form v-model=\"valid\" ref=\"formPerfil\" :lazy-validation=\"true\">\n    <thead style=\"font-size:40px;align:center;\">Registro de Perfil Ocupacional de Graduado</thead>\n    <v-divider class=\"black\"></v-divider>\n    <thead style=\"font-size:25px\">I.Datos Personales</thead>\n    <v-divider class=\"black\"></v-divider>\n    <v-container>\n      <!--v-row parte 1-->\n      <v-row>\n         <!--nombre-->\n        <v-col cols=\"12\" md=\"6\">\n          <v-text-field\n            :label=\"arrayAspirante.nombres\"\n            disabled\n          ></v-text-field>\n        </v-col>\n        <!--Apellido-->\n        <v-col cols=\"12\" md=\"6\">\n          <v-text-field\n            :label=\"arrayAspirante.apellidos\"\n            disabled\n          ></v-text-field>\n        </v-col>\n        <!--datepicker de nacimiento-->\n        <v-col cols=\"12\" md=\"6\">\n          <v-text-field\n            :label=\"arrayAlumno.fecha_nac\"\n            disabled\n          ></v-text-field>\n        </v-col>\n      <!--lugar de nacimiento-->\n      <v-col cols=\"12\" md=\"6\">\n          <v-text-field\n            v-model=\"formulario.lugar_nac\"\n            :rules=\"bornRules\"\n            label=\"Lugar de Nacimiento\"\n            required\n          ></v-text-field>\n        </v-col>\n        <!--Direccion-->\n      <v-col cols=\"12\" md=\"6\">\n          <v-text-field\n            :label=\"arrayAlumno.direccion\"\n            disabled\n          ></v-text-field>\n        </v-col>\n        <!--estado civil-->\n      <v-col cols=\"12\" md=\"2\">\n           <v-select\n           v-model=\"formulario.idestado_civil\"\n          :items=\"arrayEstadoCivil\"\n          label=\"Estado Civil\"\n          item-text=\"nombre\"\n          item-value=\"id\"\n          clearable\n          :menu-props=\"{ closeOnClick: true }\"\n        ></v-select>\n        </v-col>\n        <!--Numero de telefono-->\n      <v-col cols=\"12\" md=\"2\">\n          <v-text-field\n           :label=\"arrayAlumno.celular\"\n           disabled\n          ></v-text-field>\n        </v-col>\n        <v-tooltip bottom>\n        <template v-slot:activator=\"{ on, attrs }\">\n        <v-icon\n          color=\"primary\"\n          dark\n          v-bind=\"attrs\"\n          v-on=\"on\"\n        >\n          mdi-info\n        </v-icon>\n      </template>\n      <span>Tooltip</span>\n      </v-tooltip>\n        <v-col cols=\"12\" md=\"2\">\n          <v-text-field\n            v-model=\"formulario.celular2\"\n            :rules=\"phoneRules\"\n            label=\"Nuevo Numero\"\n          ></v-text-field>\n        </v-col>\n        <!--DUI-->\n        <v-col cols=\"12\" md=\"3\">\n          <v-text-field\n            :label=\"arrayAlumno.dui\"\n            disabled\n          ></v-text-field>\n        </v-col>\n        <!--NIT-->\n        <v-col cols=\"12\" md=\"2\">\n          <v-text-field\n            v-model=\"formulario.nit\"\n            :rules=\"nitRules\"\n            label=\"Nit\"\n            required\n          ></v-text-field>\n        </v-col>\n          <!--Pasaporte-->\n        <v-col cols=\"12\" md=\"2\">\n          <v-text-field\n            v-model=\"formulario.pasaporte\"\n            :rules=\"passportRules\"\n            label=\"Pasaporte\"\n            required\n          ></v-text-field>\n        </v-col>\n         <v-col cols=\"12\" md=\"2\">\n          <v-text-field\n            v-model=\"formulario.licencia_conducir\"\n            :rules=\"nitRules\"\n            label=\"Nit\"\n            required\n          ></v-text-field>\n        </v-col>\n         <!--NUP-->\n        <v-col cols=\"12\" md=\"3\">\n          <v-text-field\n            v-model=\"formulario.nup\"\n            :rules=\"nupRules\"\n            label=\"NUP\"\n            required\n          ></v-text-field>\n        </v-col>\n         <!--Nacionalidadd-->\n        <v-col cols=\"12\" md=\"4\">\n          <v-text-field\n            v-model=\"formulario.nacionalidad\"\n            :rules=\"nacionRules\"\n            label=\"Nacionalidad\"\n            required\n          ></v-text-field>\n        </v-col>\n        <!--2 Idioma-->\n        <v-col cols=\"12\" md=\"4\">\n          <v-autocomplete\n            v-model=\"formulario.idsegundo_idioma\"\n            :items = \"arrayIdioma\"\n            label=\"Segundo Idioma\"\n            item-text=\"nombre\"\n            item-value=\"id\"\n            clearable\n            :menu-props=\"{ closeOnClick: true }\"\n          ></v-autocomplete>\n        </v-col>\n        <!--nivel de Idioma-->\n        <v-col class=\"d-flex\" cols=\"12\" md=\"4\">\n        <v-select\n          v-model=\"formulario.nivel_idioma\"\n          :items=\"items\"\n          label=\"Nivel de Segundo Idioma\"\n        ></v-select>\n        </v-col>\n      </v-row>\n      <v-divider class=\"black\"></v-divider>\n      <thead style=\"font-size:25px\">II.Estado De Salud</thead>\n      <v-divider class=\"black\"></v-divider>\n      <!--v-row parte 2-->\n      <v-row>\n      <!--Enfermemdad Cronica-->\n        <v-col cols=\"12\" md=\"6\">\n          <v-select\n          \n            v-model=\"formulario.enfermadad_mencion\"\n            :items=\"yesno\"\n            label=\"Padece alguna Enfermedad Cronica?\"\n          ></v-select>\n        </v-col>\n         <!--si?Enfermemdad Cronica-->\n        <v-col cols=\"12\" md=\"6\">\n          <v-autocomplete\n           :disabled=\"formulario.enfermadad_mencion==='No'\" \n            v-model=\"formulario.idenfermedad_cronica\"\n            :items=\"arrayEnfermedad\"\n            label=\"Si? Seleccione\"\n            item-text=\"nombre\"\n            item-value=\"id\"\n            clearable\n            :menu-props=\"{ closeOnClick: true }\"\n          ></v-autocomplete>\n        </v-col>\n        <!--Medicamentos-->\n        <v-col cols=\"12\" md=\"6\">\n          <v-select\n            v-model=\"formulario.medicamento_perma\"\n           :items=\"yesno\"\n            label=\"Necesita Medicamentos Permanentes?\"\n          ></v-select>\n        </v-col>\n         <!--Si?Medicamentos-->\n        <v-col cols=\"12\" md=\"6\">\n          <v-text-field\n            v-model=\"formulario.medicamento_mencion\"\n            :disabled=\"formulario.medicamento_perma==='No'\"\n            label=\"Si? Mencionar\"\n          ></v-text-field>\n        </v-col>\n        <!--Discapacidad-->\n        <v-col cols=\"12\" md=\"12\">\n          <v-select\n            v-model=\"formulario.discapacidad\"\n            :items=\"yesno\"\n            label=\"Posee Algun Tipo de Discapacidad?\"\n          ></v-select>\n        </v-col>\n      </v-row>\n      <v-divider class=\"black\"></v-divider>\n      <thead style=\"font-size:25px\">III.Informacion Academica</thead>\n      <v-divider class=\"black\"></v-divider>\n      <!--v-row parte 3-->\n      <v-row>\n        <!--nivel academico-->\n        <v-col cols=\"12\" md=\"3\">\n          <v-text-field\n            v-model=\"formulario.nivel_academico\"\n            label=\"Nivel Academico\"\n          ></v-text-field>\n        </v-col>\n         <!--institucion-->\n        <v-col cols=\"12\" md=\"3\">\n          <v-text-field\n            v-model=\"formulario.institucion_formadora\"\n            label=\"Institucion Formadora\"\n          ></v-text-field>\n        </v-col>\n        <!--Graduacion-->\n        <v-col cols=\"12\" md=\"2\">\n          <v-select\n            v-model=\"formulario.anio_graduacion\"\n            :items=\"anios\"\n            label=\"Año de Graduacion?\"\n          ></v-select>\n        </v-col>\n         <!--Practica Pro-->\n        <v-col class=\"d-flex\" cols=\"12\" md=\"4\">\n        <v-select\n          v-model=\"formulario.practica_pro\"\n          :items=\"yesno\"\n          label=\"Ha Realizado Practica Profecional?\"\n        ></v-select>\n        </v-col>\n        <!--Cursos-->\n        <v-col cols=\"12\" md=\"12\">\n        <v-textarea\n          v-model=\"formulario.cursos_informacion\"\n          name=\"txaCurso\"\n          label=\"Cursos de Informacion\"\n          value=\"\"\n          hint=\"Escriba Los Cursos\"\n          ></v-textarea>\n          </v-col>\n          <!--Oficios-->\n          <v-col cols=\"12\" md=\"12\">\n          <v-textarea\n            v-model=\"formulario.oficios_realizar\"\n            name=\"txaOficio\"\n            label=\"Oficios que Puede Realizar\"\n            value=\"\"\n            hint=\"Escriba Los Oficios\"\n          ></v-textarea>\n          </v-col>\n          <!--Formacion empre-->\n          <v-col class=\"d-flex\" cols=\"12\" md=\"4 \">\n          <v-select\n            v-model=\"formulario.formacion_emprende\"\n            :items=\"yesno\"\n            label=\"Formacion de Emprendimiento?\"\n          ></v-select>\n          </v-col>\n          <!--institucion emprende-->\n          <v-col cols=\"12\" md=\"4\">\n            <v-text-field\n            :disabled=\"formulario.formacion_emprende==='No'\"\n              v-model=\"formulario.instituto_formador_emprede\"\n              label=\"Institucion formadora de Emprendimiento\"\n            ></v-text-field>\n          </v-col>\n          <!--anio de formacion-->\n          <v-col cols=\"12\" md=\"2\">\n        